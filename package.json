{
  "name": "csgpro.com",
  "version": "1.0.0",
  "description": "Corporate Website",
  "main": "index.js",
  "engines": {
    "node": "6.3.0",
    "npm": "3.10.3"
  },
  "scripts": {
    "start": "node index.js",
    "postinstall": "npm run build",
    "prepare-build": "npm run clean",
    "build": "npm run prepare-build && npm run lint && npm run lib-js && npm run browser-js && npm run sass && npm run public && npm run views && npm run hbs && npm run downloads",
    "clean": "rimraf ./dist/* !./dist/.gitkeep",
    "clean-deep": "rimraf ./src/**/*.js ./test/**/*.js ./utils/**/*.js, ./dist/* !./dist/.gitkeep !./src/public/scripts/*.js",
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "lib-js": "tsc",
    "lib-js:watch": "tsc --watch",
    "browser-js": "webpack",
    "browser-js:watch": "webpack --watch",
    "sass": "node dist/bin/compile-sass.js",
    "sass:watch": "node dist/bin/compile-sass.js --watch",
    "public": "node dist/bin/copy-files.js --outDir dist/public --srcDir src/public",
    "public:watch": "node dist/bin/copy-files.js --outDir dist/public --srcDir src/public --watch",
    "views": "node dist/bin/copy-files.js --ext html --outDir dist/views --srcDir src/views",
    "views:watch": "node dist/bin/copy-files.js --ext html --outDir dist/views --srcDir src/views --watch",
    "hbs": "node dist/bin/copy-files.js --ext hbs --outDir dist --srcDir src",
    "hbs:watch": "node dist/bin/copy-files.js --ext hbs --outDir dist --srcDir src --watch",
    "downloads": "node dist/bin/copy-files.js --outDir dist/downloads --srcDir src/downloads",
    "downloads:watch": "node dist/bin/copy-files.js --outDir dist/downloads --srcDir src/downloads --watch",
    "test": "mocha dist/test",
    "watch": "concurrently \"npm run lint\" \"npm run lib-js:watch\" \"npm run browser-js:watch\" \"npm run sass:watch\" \"npm run public:watch\" \"npm run views:watch\" \"npm run hbs:watch\" \"npm run downloads:watch\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/csgpro/website.git"
  },
  "author": "Ken Howard <kenh@csgpro.com>",
  "license": "ISC",
  "homepage": "https://github.com/csgpro/website#readme",
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "@angular/upgrade": "2.0.0-rc.4",
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^1.1.1",
    "azure-storage": "^1.1.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.4.1",
    "boom": "^3.2.2",
    "copy-webpack-plugin": "^3.0.1",
    "core-js": "^2.4.1",
    "crypto": "0.0.3",
    "css-loader": "^0.23.1",
    "good": "^7.0.1",
    "good-console": "^6.1.2",
    "good-squeeze": "^4.0.0",
    "handlebars": "^4.0.5",
    "handlebars-paginate": "^0.1.0",
    "hapi": "^13.5.0",
    "hapi-auth-jwt2": "7.0.1",
    "highlight.js": "^9.5.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.21.0",
    "ie-shim": "^0.1.0",
    "inert": "^4.0.1",
    "json-loader": "^0.5.4",
    "jsonwebtoken": "^7.1.6",
    "lodash": "^4.2.1",
    "marked": "^0.3.5",
    "moment": "^2.14.1",
    "nconf": "^0.8.4",
    "nodemailer": "^2.5.0",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "raw-loader": "^0.5.1",
    "request": "^2.73.0",
    "rxjs": "5.0.0-beta.6",
    "sass-loader": "^4.0.0",
    "script-loader": "^0.7.0",
    "sequelize": "^3.23.6",
    "sinon": "^1.17.4",
    "source-map-loader": "^0.1.5",
    "striptags": "^2.1.1",
    "tedious": "^1.14.0",
    "to-string-loader": "^1.1.4",
    "ts-helpers": "^1.1.1",
    "tslint": "^3.14.0",
    "tslint-loader": "^2.1.5",
    "umzug": "^1.11.0",
    "vision": "^4.1.0",
    "zone.js": "~0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.3.0",
    "copy": "^0.2.0",
    "cz-conventional-changelog": "^1.1.6",
    "foundation-sites": "^6.2.1",
    "hound": "^1.0.4",
    "jquery": "^2.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "motion-ui": "^1.2.2",
    "node-sass": "^3.4.2",
    "rimraf": "^2.5.2",
    "ts-loader": "^0.8.0",
    "typescript": "^1.8.10",
    "uglify": "^0.1.5",
    "uglify-loader": "^1.3.0",
    "webpack": "^1.12.13"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
