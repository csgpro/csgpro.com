{
  "name": "csgpro.com",
  "version": "1.0.0",
  "description": "Corporate Website",
  "main": "app.js",
  "engines": {
    "node": "4.2.4",
    "npm": "3.5.3"
  },
  "scripts": {
    "start": "node app.js",
    "prepublish": "npm run clean && npm run build",
    "test": "mocha",
    "build": "npm run server-js && npm run sass && npm run client-js",
    "clean": "rimraf ./*.js src/**/*.js src/**/*.js.map migrations/**/*.js seeders/**/*.js scripts/**/*.js test/**/*.js utis/**/*.js",
    "server-js": "tsc --rootDir src",
    "server-js:watch": "tsc --watch --rootDir src",
    "sass": "node utils/compile-sass.js",
    "sass:watch": "node utils/compile-sass.js --watch",
    "client-js": "webpack --config webpack.config.js",
    "client-js:watch": "webpack --watch --config webpack.config.js",
    "prepare-deploy": "node prepare-deploy.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/csgpro/website.git"
  },
  "author": "Ken Howard <kenh@csgpro.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/csgpro/website/issues"
  },
  "homepage": "https://github.com/csgpro/website#readme",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.2.2",
    "boom": "^3.1.1",
    "good": "^6.4.0",
    "good-console": "^5.2.0",
    "handlebars": "^4.0.5",
    "handlebars-paginate": "^0.1.0",
    "hapi": "^11.1.4",
    "hapi-sitemap": "^1.0.3",
    "highlight.js": "^9.1.0",
    "inert": "^3.2.0",
    "lodash": "^4.2.1",
    "marked": "^0.3.5",
    "moment": "^2.11.2",
    "nconf": "^0.8.4",
    "nodemailer": "^2.2.1",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "script-loader": "^0.6.1",
    "sequelize": "^3.19.2",
    "sinon": "^1.17.3",
    "striptags": "^2.1.1",
    "tedious": "^1.14.0",
    "umzug": "^1.8.0",
    "vision": "^4.0.1"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "chai-as-promised": "^5.3.0",
    "cz-conventional-changelog": "^1.1.6",
    "foundation-sites": "^6.2.1",
    "hound": "^1.0.4",
    "jquery": "^2.2.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "rimraf": "^2.5.2",
    "ts-loader": "^0.8.0",
    "typescript": "^1.8.10",
    "uglify": "^0.1.5",
    "uglify-loader": "^1.3.0",
    "webpack": "^1.12.13"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
