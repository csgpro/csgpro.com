{
  "name": "csgpro.com",
  "version": "2.1.2",
  "description": "Corporate Website",
  "main": "index.js",
  "engines": {
    "node": "6.7.0",
    "npm": "3.10.3"
  },
  "scripts": {
    "start": "node index.js",
    "postinstall": "npm run build",
    "prepare-build": "npm run clean",
    "build": "npm run prepare-build && npm run lint && npm run lib-js && npm run browser-js && npm run sass && npm run public && npm run views && npm run hbs && npm run downloads",
    "clean": "rimraf ./dist/* !./dist/.gitkeep",
    "clean-deep": "rimraf ./src/**/*.js ./test/**/*.js ./utils/**/*.js, ./dist/* !./dist/.gitkeep !./src/public/scripts/*.js",
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "lib-js": "tsc",
    "lib-js:watch": "tsc --watch",
    "browser-js": "webpack",
    "browser-js:watch": "webpack --watch",
    "sass": "node dist/bin/compile-sass.js",
    "sass:watch": "node dist/bin/compile-sass.js --watch",
    "public": "node dist/bin/copy-files.js --outDir dist/public --srcDir src/public",
    "public:watch": "node dist/bin/copy-files.js --outDir dist/public --srcDir src/public --watch",
    "views": "node dist/bin/copy-files.js --ext html --outDir dist/views --srcDir src/views",
    "views:watch": "node dist/bin/copy-files.js --ext html --outDir dist/views --srcDir src/views --watch",
    "hbs": "node dist/bin/copy-files.js --ext hbs --outDir dist --srcDir src",
    "hbs:watch": "node dist/bin/copy-files.js --ext hbs --outDir dist --srcDir src --watch",
    "downloads": "node dist/bin/copy-files.js --outDir dist/downloads --srcDir src/downloads",
    "downloads:watch": "node dist/bin/copy-files.js --outDir dist/downloads --srcDir src/downloads --watch",
    "test": "mocha dist/test",
    "watch": "concurrently \"npm run lint\" \"npm run lib-js:watch\" \"npm run browser-js:watch\" \"npm run sass:watch\" \"npm run public:watch\" \"npm run views:watch\" \"npm run hbs:watch\" \"npm run downloads:watch\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/csgpro/website.git"
  },
  "author": "Ken Howard <kenh@csgpro.com>",
  "license": "ISC",
  "homepage": "https://github.com/csgpro/website#readme",
