extends layout

block content
  div.mobile-nav.hide-desk(role='navigation')
    ul
      li: a(href='#work') work /
      li: a(href='#services') services /
      li: a(href='#about') about /
      li: a(href='#updates') updates /
      li: a(href='#contact') contact /

